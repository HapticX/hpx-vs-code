{
  "name": "happyx",
  "displayName": "HappyX",
  "description": "HappyX web framework for Visual Studio Code",
  "version": "1.0.0",
  "publisher": "HapticX",
  "engines": {
    "vscode": "^1.79.2"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "happyx",
      "aliases": ["HappyX", "happyx", "hpx"],
      "extensions": [".nim",".nims"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "happyx",
      "scopeName": "source.nim",
      "path": "./syntaxes/happyx.tmLanguage.json",
      "embeddedLanguages": {
        "meta.embedded.block.javascript": "javascript",
        "meta.embedded.block.c": "c",
        "meta.embedded.block.markdown": "markdown",
        "meta.embedded.block.cpp": "cpp",
        "meta.embedded.block.asm": "asm"
      }
    }],
    "snippets": [{
      "language": "happyx",
      "path": "./snippets/snippets.json"
    }],
    "activationEvents": [
        "onLanguage:nim"
    ],
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
      "@types/node": "^8.10.25",
      "@types/vscode": "^1.51.0",
      "tslint": "^5.16.0",
      "typescript": "^3.4.5"
    }
  }
}
